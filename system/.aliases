# Aliases
alias py="python"
alias ipy="ipython"
alias k9="kill -9"
alias brew-upg="brew update && brew upgrade && brew cleanup && brew cask cleanup"

# mongodb
alias moup="brew services start mongodb"
alias modw="brew services stop mongodb"

# weather
alias wttr="curl http://wttr.in/muc"

# decrypt from clipboard with gpg
alias dcr="pbpaste > /tmp/msg.asc && gpg -o - /tmp/msg.asc; rm /tmp/msg.asc"
